  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "format": "prettier -w src/js/**/*.js",
    "lint": "eslint src/**/*.js",
